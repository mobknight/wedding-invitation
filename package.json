		"typescript": "^5.0.0",
		"vite": "^5.0.3"
	},
	"type": "module",
	"dependencies": {
		"@jamescoyle/svelte-icon": "^0.1.1",
		"@mdi/js": "^7.4.47",
		"animate.css": "^4.1.1",
